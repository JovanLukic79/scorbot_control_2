<?xml version="1.0"?>
<robot name="scorbot">



    <!-- WORLD LINK -> Used for fixing robot to Gazebo base_link' -->
    <link name="world"/>

    <joint name="scorbot_base_joint" type="fixed">
      <parent link="world"/>
      <child link="scorbot_base_link"/>
    </joint>

    <!--BASE LINK-->
    <!--Bounding box: 0.24 0.24 0.2274-->
    <link name="scorbot_base_link">
      <visual>
        <origin
        xyz="0.0 0.0 0.11"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://scorbot_control_2/meshes/base.DAE"/>
        </geometry>
      </visual>
    </link>

    <!--Link 1 SHOULDER LINK-->
    <!--Bounding box: 0.205 0.26 0.17-->
    <link name="scorbot_link_1">
      <visual>
        <origin
          xyz="0.0 0.0 0.075"
          rpy="0 0 0" />      
        <geometry>
          <mesh filename="package://scorbot_control_2/meshes/shoulder.DAE"/>
        </geometry>
      </visual>
    </link>

    <!--Joint 1 BASE JOINT-->
    <joint name="scorbot_joint_1" type="revolute">
      <parent link="scorbot_base_link"/>
      <child link="scorbot_link_1"/>
      <origin xyz="0 0 0.2274" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="-3.1415926535897931" upper="3.1415926535897931" effort="1000" velocity="2"/>
    </joint>


    <!--Link 2 ELBOW LINK--> 
    <!-- Bounding box: 0.28749 0.069982 0.169 -->
    <link name="scorbot_link_2">
      <visual>
        <origin
          xyz="0.11 0.0 0.0"
          rpy="0 0 0" />      
        <geometry>
          <mesh filename="package://scorbot_control_2/meshes/elbow.DAE"/>
        </geometry>
      </visual>
    </link>

    <!--Joint 2 SHOULDER JOINT-->
    <joint name="scorbot_joint_2" type="revolute">
      <parent link="scorbot_link_1"/>
      <child link="scorbot_link_2"/>
      <origin xyz="0.0255 0 0.13" rpy="-1.570796327 -1.570796327 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="-0.610865" upper="2.268928" effort="1000" velocity="2"/>
      <!-- <dynamics
        damping="0.0"
        friction="0.5" /> -->
    </joint> 

    <!--Link 3 PITCH LINK-->
    <!-- Bounding box: 0.289918 0.06 0.15 -->
    <link name="scorbot_link_3">
      <visual>
        <origin
          xyz="0.12 0.0 0.0"
          rpy="0 0 0" />      
        <geometry>
          <mesh filename="package://scorbot_control_2/meshes/end_effector_pitch.DAE"/>
        </geometry>
      </visual>
    </link>

    <!--Joint 3 ELBOW JOINT-->
    <joint name="scorbot_joint_3" type="revolute">
      <parent link="scorbot_link_2"/>
      <child link="scorbot_link_3"/>
      <origin xyz="0.2225 0 0" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="-2.268928" upper="2.268928" effort="1000" velocity="2"/>
      <!-- <dynamics
        damping="0.0"
        friction="0.5" /> -->
    </joint>

    <!--Link 4 ROLL LINK-->
    <!-- Bounding box: 0.183046 0.073212 0.075543 -->
    <link name="scorbot_link_4">
      <visual>
        <origin rpy="0 0 0" xyz="0.012 0 0"/>
        <geometry>
          <mesh filename="package://scorbot_control_2/meshes/end_effector_roll.DAE"/>
        </geometry>
      </visual>
    </link>

    <!--Joint 4 PITCH JOINT-->
    <joint name="scorbot_joint_4" type="revolute">
      <parent link="scorbot_link_3"/>
      <child link="scorbot_link_4"/>
      <origin xyz="0.2225 0 0" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="-2.268928" upper="2.268928" effort="1000" velocity="2"/>
      <!-- <dynamics
        damping="0.0"
        friction="0.5" /> -->
    </joint>

    <!--Link 5 GRIPPER LINK-->
    <!-- Bounding box: 0.06 0.14 0.238 -->
    <link name="scorbot_link_5">
      <visual>
        <origin
          xyz="0.0 0.0 0.02"
          rpy="0 0 0" />
        <geometry>
          <mesh filename="package://scorbot_control_2/meshes/gripper.STL" />
        </geometry>
      </visual>
    </link>

    <!--Joint 5 ROLL JOINT-->
    <joint name="scorbot_joint_5" type="revolute">
      <parent link="scorbot_link_4"/>
      <child link="scorbot_link_5"/>
      <origin xyz="0 0 0" rpy="0.0 -1.570796327 0.0" />
      <axis xyz="0 0 1"/>
      <limit
        lower="-3.14159"
        upper="3.14159"
        effort="1000"
        velocity="2" />
      <!-- <dynamics damping="0.01" friction="0.5" /> -->
    </joint>


    <!--PAD1 LINK-->
    <!-- Bounding box: 0.073966 0.02 0.01 -->
    <link name="scorbot_pad1_Link">
      <visual>
        <origin
          xyz="-0.035 0.0 0.0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://scorbot_control_2/meshes/pad1.STL" />
        </geometry>
      </visual>
    </link>

    <!--PAD1 JOINT-->
    <joint  name="scorbot_pad1_joint" type="revolute">
      <origin xyz="0 -0.0545 -0.0905" rpy="1.5708 -0.74758 -1.5708" />
      <parent link="scorbot_link_5" />
      <child link="scorbot_pad1_Link" />
      <axis xyz="0 0 1" />
      <limit
        lower="0"
        upper="1.5709"
        effort="1000"
        velocity="2" />
      <!-- <dynamics damping="0.01" friction="0.01" /> -->
    </joint>

    <!--PAD2 LINK-->
    <!-- Bounding box: 0.073966 0.02 0.01 -->
    <link name="scorbot_pad2_Link">
      <visual>
        <origin
          xyz="-0.035 0.0 0.0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://scorbot_control_2/meshes/pad2.STL" />
        </geometry>
      </visual>
    </link>

    <!--PAD2 JOINT-->
    <joint name="scorbot_pad2_joint" type="revolute">
      <origin xyz="0 0.0545 -0.0905" rpy="-1.5708 -0.74758 1.5708" />
      <parent link="scorbot_link_5" />
      <child link="scorbot_pad2_Link" />
      <axis xyz="0 0 -1" />
      <limit
        lower="0"
        upper="1.5709"
        effort="1000"
        velocity="2" />
      <!-- <dynamics damping="0.01" friction="0.01" /> -->
      <mimic joint="scorbot_pad1_joint" multiplier="1.0" offset="0"/>
    </joint>





</robot>